{
  "name": "learning-oe-solidjs",
  "description": "Configurable quiz cards for language learning or other quizes",
  "private": true,
  "version": "0.2.4",
  "type": "module",
  "homepage": "https://leegee.github.io/learning-oe-solidjs",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist/* && tsc -b && vite build",
    "build:lib": "rimraf dist/* && cross-env BUILD_LIB=true tsc -b && cross-env BUILD_LIB=true vite build",
    "delint": "eslint .",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "e2e": "bun run playwright test",
    "e2e:record": "playwright codegen http://localhost:5173/learning-oe-solidjs/",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "prebuild": "npm run test"
  },
