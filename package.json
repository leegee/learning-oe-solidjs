    "ajv": "^8.17.1",
    "dexie": "^4.2.1",
    "i18next": "^24.2.3",
    "solid-js": "^1.9.10"
  },
  "resolutions": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "chalk": "4.1.2",
    "@babel/preset-env": "^7.28.5",
